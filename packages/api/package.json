{
  "name": "@halcyon-rcm/api",
  "version": "1.0.0",
  "type": "module",
  "description": "Express API server for Halcyon RCM Partner Assistant",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:prod": "node init-db.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:dev": "tsx prisma/seed-dev-login.ts",
    "db:studio": "prisma studio",
    "db:clear": "node scripts/clear-data.js",
    "db:reset": "prisma db push --force-reset && npm run db:seed:dev",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@aws-sdk/client-ses": "^3.864.0",
    "@aws-sdk/client-sns": "^3.864.0",
    "@aws-sdk/client-textract": "^3.864.0",
    "@azure/cognitiveservices-computervision": "^8.2.0",
    "@azure/ms-rest-js": "^2.7.0",
    "@google-cloud/vision": "^5.3.3",
    "@google/generative-ai": "^0.24.1",
    "@halcyon-rcm/core": "*",
    "@halcyon-rcm/file-exchange": "*",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-type": "^19.6.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.16",
    "openai": "^4.73.0",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.2",
    "pg-boss": "^10.0.0",
    "prisma": "^5.22.0",
    "ssh2-sftp-client": "^12.0.1",
    "uuid": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.17.4",
    "@types/ssh2-sftp-client": "^9.0.6",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Halcyon",
  "license": "UNLICENSED"
}
