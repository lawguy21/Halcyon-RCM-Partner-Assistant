{
  "name": "@halcyon-rcm/core",
  "version": "1.0.0",
  "description": "Core recovery engine logic for Halcyon RCM Partner Assistant",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
